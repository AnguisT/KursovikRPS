<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <managed-bean>
        <managed-bean-name>BulletinMB</managed-bean-name>
        <managed-bean-class>ManagedBens.BulletinMB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>CandidateMB</managed-bean-name>
        <managed-bean-class>ManagedBens.CandidateMB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <managed-bean>
        <managed-bean-name>PeopleMB</managed-bean-name>
        <managed-bean-class>ManagedBens.PeopleMB</managed-bean-class>
        <managed-bean-scope>session</managed-bean-scope>
    </managed-bean>
    <navigation-rule>
        <from-view-id>/Candidate/listCandidate.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Inform</from-outcome>
            <to-view-id>/Candidate/InformCandidate.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>List</from-outcome>
            <to-view-id>/Candidate/listCandidate.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Home.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Voter</from-outcome>
            <to-view-id>/National/Voter.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>List</from-outcome>
            <to-view-id>/Candidate/listCandidate.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Login</from-outcome>
            <to-view-id>/index.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/National/Voter.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/index.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Candidate/InformCandidate.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>List</from-outcome>
            <to-view-id>/Candidate/listCandidate.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
