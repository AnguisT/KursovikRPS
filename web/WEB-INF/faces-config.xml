<?xml version='1.0' encoding='UTF-8'?>
<faces-config version="2.2"
              xmlns="http://xmlns.jcp.org/xml/ns/javaee"
              xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
    <navigation-rule>
        <from-view-id>/Candidate/listCandidate.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Inform</from-outcome>
            <to-view-id>/Candidate/InformCandidate.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>List</from-outcome>
            <to-view-id>/Candidate/listCandidate.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Home.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Voter</from-outcome>
            <to-view-id>/National/Voter.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>List</from-outcome>
            <to-view-id>/Candidate/listCandidate.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>DeleteCandidate</from-outcome>
            <to-view-id>/Admin/DeleteCandidate.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>AddCandidate</from-outcome>
            <to-view-id>/Admin/AddCandidate.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/National/Voter.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Candidate/InformCandidate.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>List</from-outcome>
            <to-view-id>/Candidate/listCandidate.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Admin/DeleteCandidate.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>DeleteCandidate</from-outcome>
            <to-view-id>/Admin/DeleteCandidate.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
    <navigation-rule>
        <from-view-id>/Admin/AddCandidate.xhtml</from-view-id>
        <navigation-case>
            <from-outcome>AddCandidate</from-outcome>
            <to-view-id>/Admin/AddCandidate.xhtml</to-view-id>
        </navigation-case>
        <navigation-case>
            <from-outcome>Home</from-outcome>
            <to-view-id>/Home.xhtml</to-view-id>
        </navigation-case>
    </navigation-rule>
</faces-config>
